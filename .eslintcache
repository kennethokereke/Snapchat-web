er\"\n}\n\n\nfunction WebcamCapture() {\n    const webcamRef = useRef(null)\n    const dispatch = useDispatch()\n    const history = useHistory()\n  \n    const capture = useCallback(\n        () => {\n\n            // this gives us an image\n            const imageSrc = webcamRef.current.getScreenshot();\n            dispatch(setCameraImage(imageSrc))\n            history.push(\"/preview\")\n        \n\n        },\n        [webcamRef],\n    )\n    return (\n        <div className='webcamCapture'>\n            <Webcam\n            audio ={false}\n            height={videoContraints.height}\n            ref={webcamRef}\n            screenshotFormat=\"image/jpeg\"\n            videoConstraints={videoContraints}\n\n            />\n\n            <RadioButtonUncheckedIcon\n            className='webcamCapture__button'\n            onClick={capture}\n            fontSize='large'\n            \n            />\n           \n            \n        </div>\n    )\n}\n\nexport default WebcamCapture\n\n\n\n","/Users/kennethokereke/Desktop/snapchat-web/src/features/cameraSlice.js",[],["59","60"],"/Users/kennethokereke/Desktop/snapchat-web/src/app/store.js",[],"/Users/kennethokereke/Desktop/snapchat-web/src/Components/preview.js",[],"/Users/kennethokereke/Desktop/snapchat-web/src/Components/Chat.js",["61"],"import { Avatar, Button } from '@material-ui/core'\nimport  RadioButtonCheckedIcon from '@material-ui/icons/RadioButtonUnchecked'\nimport ChatBubbleIcon from '@material-ui/icons/ChatBubble'\nimport SearchIcon from '@material-ui/icons/Search'\nimport React, { useEffect, useState } from 'react'\nimport { useDispatch, useSelector } from 'react-redux'\nimport { auth, db } from '../Config/firebase'\nimport { selectUser } from '../features/appSlice'\nimport '../Stylesheet/Chat.css'\nimport PostChat from './PostChat'\nimport { useHistory } from 'react-router-dom'\nimport { reseatCameraImage } from '../features/cameraSlice'\n\nfunction Chat() {\n    const [posts, setPosts] = useState([]);\n    const user = useSelector(selectUser)\n    const dispatch = useDispatch()\n    const history = useHistory()\n\n\n    useEffect(() => {\n        //give me the latest database snapchat\n        db.collection(\"posts\").orderBy('timestamp', 'desc').onSnapshot(snapshot =>\n            setPosts(snapshot.docs.map(doc => ({\n                id: doc.id,\n                data: doc.data(),\n\n            }))))\n\n    }, []\n    )\n\n    const takeSnap = () => {\n        dispatch(reseatCameraImage())\n        history.push(\"/\")\n\n\n}\n\nconst logouts = () => {\n    auth.signOut().then(()=> {\n        console.log('logged out')\n        history.push('/')\n\n        \n      }).catch((error) => {\n        console.log(error.message)\n      })\n  \n}\n    return (\n        \n\n       \n        <div className=\"chats\">\n            <div className=\"chats__header\">\n                <div>\n                     <Avatar \n                src={user.profilePic} \n                onClick={logouts} \n                className=\"chats__avatar\"/>\n                </div>\n\n                \n                <div className=\"chats__search\">\n                    <SearchIcon className=\"chats__searchIcon\" />\n                    <input placeholder='Friends' type=\"text\" />\n                </div>\n                <ChatBubbleIcon className=\"chats__chatIcon\" />\n            </div>\n            <div className=\"chats__posts\" >\n              {posts.map(({id, data: {profilePic, username, timestamp, imageUrl,read}}) => \n              (\n                    <PostChat\n                    key={id}\n                    id={id}\n                    username={username}\n                    timestamp={timestamp}\n                    imageUrl={imageUrl}\n                    read={read}\n                    profilePic={profilePic}\n                    />\n\n              ))}\n            </div>\n            <RadioButtonCheckedIcon\n            className='chats__takePicIcon'\n            onClick={takeSnap}\n            fontSize='large'\n            />\n\n           \n            \n        </div>\n        \n        \n\n\n    )\n}\n\nexport default Chat\n\n","/Users/kennethokereke/Desktop/snapchat-web/src/Components/PostChat.js",[],"/Users/kennethokereke/Desktop/snapchat-web/src/Components/ChatView.js",["62"],"/Users/kennethokereke/Desktop/snapchat-web/src/Config/firebase.js",[],"/Users/kennethokereke/Desktop/snapchat-web/src/Components/Login.js",[],{"ruleId":"63","severity":1,"message":"64","line":3,"column":25,"nodeType":"65","messageId":"66","endLine":3,"endColumn":29},{"ruleId":"67","severity":1,"message":"68","line":32,"column":6,"nodeType":"69","endLine":32,"endColumn":8,"suggestions":"70"},{"ruleId":"71","replacedBy":"72"},{"ruleId":"73","replacedBy":"74"},{"ruleId":"67","severity":1,"message":"75","line":31,"column":9,"nodeType":"69","endLine":31,"endColumn":20,"suggestions":"76"},{"ruleId":"71","replacedBy":"77"},{"ruleId":"73","replacedBy":"78"},{"ruleId":"63","severity":1,"message":"79","line":1,"column":18,"nodeType":"65","messageId":"66","endLine":1,"endColumn":24},{"ruleId":"67","severity":1,"message":"80","line":18,"column":8,"nodeType":"69","endLine":18,"endColumn":23,"suggestions":"81"},"no-unused-vars","'Link' is defined but never used.","Identifier","unusedVar","react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'dispatch'. Either include it or remove the dependency array.","ArrayExpression",["82"],"no-native-reassign",["83"],"no-negated-in-lhs",["84"],"React Hook useCallback has missing dependencies: 'dispatch' and 'history'. Either include them or remove the dependency array.",["85"],["83"],["84"],"'Button' is defined but never used.","React Hook useEffect has a missing dependency: 'exit'. Either include it or remove the dependency array.",["86"],{"desc":"87","fix":"88"},"no-global-assign","no-unsafe-negation",{"desc":"89","fix":"90"},{"desc":"91","fix":"92"},"Update the dependencies array to be: [dispatch]",